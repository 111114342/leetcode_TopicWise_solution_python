# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, data=0, next=None):
#         self.data = data
#         self.next = next
class Solution:
    def deleteDuplicates(self, head: ListNode) -> ListNode:
        temp = head
        if temp is None:
            return
        while temp.next is not None:
            if temp.val == temp.next.val:
                new = temp.next.next
                temp.next = None
                temp.next = new
            else:
                temp = temp.next
        return head
