/*Richest Customer Wealth*/
class Solution:
    def shuffle(self, nums: List[int], n: int) -> List[int]:
        l=0
        r=n
        l1=[]
        while(l!=len(nums)//2):
            l1.append(nums[l])
            l1.append(nums[r])
            l=l+1
            r=r+1
        return l1
            
            
        
