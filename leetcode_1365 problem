/*How Many Numbers Are Smaller Than the Current Number*/


class Solution:
    def smallerNumbersThanCurrent(self, nums: List[int]) -> List[int]:
        l=[]
        for i in range(len(nums)):
            count=0
            for j in range(len(nums)):
                if(j!=i and nums[j]<nums[i]):
                    count=count+1
            l.append(count)
        return l
